// Session Module style

body::after{
   position:absolute; width:0; height:0; overflow:hidden; z-index:-1;
   content: url('/resources/lmn_common/resources/img/course_card_colored_darkblue_trans.png')
    url('/resources/lmn_common/resources/img/course_card_colored_orange_trans.png')
    url('/resources/lmn_common/resources/img/course_people_colored_darkblue_trans.png')
    url('/resources/lmn_common/resources/img/course_people_colored_orange_trans.png')
    url('/resources/lmn_common/resources/img/course_class_colored_darkblue_trans.png')
    url('/resources/lmn_common/resources/img/course_class_colored_orange_trans.png');
}

.session_container {
  padding-top: 5px;
}

.session_btn {
  border: 1px solid #eeeeee;
  background-color: white;
  font-size: 18px;
  height: 30px;
  width: 30px;
  color: #233c4c;
  padding: 5px;
  margin-top: 3px;
  margin-right: 2px;
  box-shadow: 3px 3px 3px silver;
}

.session_hr {
  border-bottom: 1px solid #eeeeee;
  width:100%;
}

.session_image {
  height: 75px;
  width: 75px;
  margin: 5px;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: 5px 10px;
  background-size: 65px 50px;
  border: 1px solid #eeeeee;
  box-shadow: 3px 3px 3px silver;
  padding: 10px;
}

.session_image:hover {
  color: #333333;
  background-color: #e6e6e6;
  border-color: #cfcfcf;
}

.session_image_sessions {
  background-image: url('/resources/lmn_common/resources/img/course_card_colored_darkblue_trans.png');
}

.session_image_sessions:hover {
  background-image: url('/resources/lmn_common/resources/img/course_card_colored_orange_trans.png');
}

.session_image_classes {
  background-size: 55px 50px;
  background-position: 10px 10px;
  background-image: url('/resources/lmn_common/resources/img/course_people_colored_darkblue_trans.png');
}

.session_image_classes:hover {
  background-size: 55px 50px;
  background-position: 10px 10px;
  background-image: url('/resources/lmn_common/resources/img/course_people_colored_orange_trans.png');
}

.session_image_room {
  background-size: 55px 50px;
  background-position: 10px 10px;
  background-image: url('/resources/lmn_common/resources/img/course_class_colored_darkblue_trans.png');
}

.session_image_room:hover {
  background-size: 55px 50px;
  background-position: 10px 10px;
  background-image: url('/resources/lmn_common/resources/img/course_class_colored_orange_trans.png');
}

.session_image_btn {
  border: 1px solid #eeeeee;
  background-color: white;
  font-size: 20px;
  height: 35px;
  width: 35px;
  color: #233c4c;
  padding: 6px;
  position: absolute;
  top: 55px;
  left: 60px;
}
